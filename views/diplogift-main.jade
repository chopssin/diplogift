extends layout

block stylesheets
  link( rel="stylesheet" href = "/stylesheets/diplogift.render.css" )

block content
  #nav.nav.fixed.bg-brown(style="opacity:0.98")
    a.logo(href="/")
  #page-container

block javascripts
  script( src = "https://www.gstatic.com/firebasejs/3.5.0/firebase.js" )
  script( src = "https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js" )
  script( src = "/javascripts/copeinit.js" )
  script( src = "/javascripts/render-page.js")
  if editable
    script.
      Dg.build({
        editable: #{editable},
        appName: '#{appName}',
        path: '#{path}',
        graphId: '#{graphId}',
        page: '#{page}'
      });
  else 
    script.
      Dg.build({
        appName: '#{appName}',
        path: '#{path}',
        graphId: '#{graphId}',
        page: '#{page}'
      });
